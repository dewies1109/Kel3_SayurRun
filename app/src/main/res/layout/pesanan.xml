<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginBottom="80dp">
            <LinearLayout
                android:id="@+id/tit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="24dp"
                android:paddingLeft="24dp"
                android:paddingRight="24dp">
                <ImageButton
                    android:layout_width="19.47dp"
                    android:layout_height="11.15dp"
                    android:layout_marginTop="14dp"
                    android:background="@drawable/back"/>
                <TextView
                    android:layout_width="343dp"
                    android:layout_height="wrap_content"
                    android:text="Detail Pembelian"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="18dp"
                    android:textAlignment="center"
                    android:textColor="#0C2E0B"
                    android:layout_marginTop="6dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/li"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:orientation="vertical"
                android:paddingLeft="24dp"
                android:paddingRight="24dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/paket"
                    android:paddingTop="24dp"
                    android:paddingBottom="24dp"
                    android:paddingRight="24dp"
                    android:paddingLeft="24dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="88.72dp"
                            android:layout_height="59dp"
                            android:layout_marginTop="15dp"
                            android:src="@drawable/tom"/>
                        <LinearLayout
                            android:layout_width="113dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="24dp">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Tomat"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#564734"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Tomato"
                                android:fontFamily="@font/poppins_italic"
                                android:textSize="12dp"
                                android:textColor="#564734"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="+     5 kg     -"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#0C2E0B"
                                android:layout_marginTop="19dp"/>
                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ImageButton
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:background="@drawable/x"
                                android:layout_alignParentRight="true"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Rp 75.000"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#0C2E0B"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"/>
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="@drawable/paket"
                    android:paddingTop="24dp"
                    android:paddingBottom="24dp"
                    android:paddingRight="24dp"
                    android:paddingLeft="24dp"
                    android:layout_marginTop="24dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="88.72dp"
                            android:layout_height="59dp"
                            android:layout_marginTop="15dp"
                            android:src="@drawable/bwg"/>
                        <LinearLayout
                            android:layout_width="113dp"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="24dp">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Bawang Merah"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#564734"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Onion"
                                android:fontFamily="@font/poppins_italic"
                                android:textSize="12dp"
                                android:textColor="#564734"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="+     3 kg     -"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#0C2E0B"
                                android:layout_marginTop="19dp"/>
                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <ImageButton
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:background="@drawable/x"
                                android:layout_alignParentRight="true"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Rp 54.000"
                                android:fontFamily="@font/poppins_bold"
                                android:textSize="14dp"
                                android:textColor="#0C2E0B"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"/>
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/tot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24dp"
                    android:layout_marginRight="24dp"
                    android:layout_alignParentBottom="true">
                    <TextView
                        android:id="@+id/strtot"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tot"
                        android:fontFamily="@font/poppins_bold"
                        android:textSize="16dp"
                        android:textColor="#564734"
                        android:layout_alignParentRight="true"
                        android:layout_marginBottom="24dp"/>
                    <Button
                        android:layout_below="@id/strtot"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@drawable/beli"
                        android:text="Pesan Sekarang"
                        android:textColor="@color/white"
                        android:textAllCaps="false"
                        android:layout_marginBottom="24dp"/>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@drawable/menu"
            android:layout_alignParentBottom="true">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageButton
                        android:layout_width="32dp"
                        android:layout_height="35dp"
                        android:src="@drawable/brn"
                        android:layout_marginLeft="24dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/brn"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Beranda"
                        android:fontFamily="@font/poppins_medium"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:layout_marginLeft="19dp"
                        android:layout_marginTop="1dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageButton
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/produk"
                        android:layout_marginLeft="68dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/produk"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sayuran"
                        android:fontFamily="@font/poppins_medium"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:layout_marginLeft="65dp"
                        android:layout_marginTop="1dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageButton
                        android:layout_width="36dp"
                        android:layout_height="32.4dp"
                        android:src="@drawable/mart"
                        android:layout_marginLeft="72dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/mart"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/pesanan"
                        android:fontFamily="@font/poppins_medium"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:layout_marginLeft="68dp"
                        android:layout_marginTop="1dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <ImageButton
                        android:layout_width="36dp"
                        android:layout_height="36dp"
                        android:src="@drawable/profil"
                        android:layout_marginLeft="70dp"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/profil"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Profil"
                        android:fontFamily="@font/poppins_medium"
                        android:textColor="@color/white"
                        android:textSize="10dp"
                        android:layout_marginLeft="75dp"
                        android:layout_marginTop="1dp"/>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>